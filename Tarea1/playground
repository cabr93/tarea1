//: Playground - noun: a place where people can play

import UIKit

for n in 0...100{
    if (n/5)*5 == n{
        print("\(n)\tBingo!!!")
    }
    if (n/2)*2 == n{
        print("\(n)\tpar!!!")
    }
    else{
        print("\(n)\timpar!!!")
    }
    if n >= 30 && n <= 40{
        print("\(n)\tVivaSwift!!!")
    }
}
